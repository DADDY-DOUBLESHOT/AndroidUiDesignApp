<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ExpandableLayout">
        <attr name="duration" format="integer"/>
        <attr name="clickToToggle" format="boolean"/>
        <attr name="initialState" format="enum" >
            <enum name="expand" value="1" />
            <enum name="collapse" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="SmoothCheckBox">
        <attr name="durationC" format="integer"/>
        <attr name="stroke_width" format="dimension"/>
        <attr name="color_tick" format="color"/>
        <attr name="color_checked" format="color"/>
        <attr name="color_unchecked" format="color"/>
        <attr name="color_unchecked_stroke" format="color"/>
    </declare-styleable>


    <declare-styleable name="ExpandablePager">
        <attr name="animation_duration" format="integer" />
        <attr name="collapsed_height" format="dimension" />
    </declare-styleable>

    <item name="internal_page_id" type="id" />
    <item name="internal_pager_id" type="id" />


</resources>